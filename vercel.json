{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "python3.9",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "DEEPL_API_KEY": "@deepl_api_key",
    "SECRET_KEY": "@secret_key",
    "FLASK_ENV": "production"
  }
}